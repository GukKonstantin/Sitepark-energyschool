CREATE TABLE `s_students` (
  `id` int(100) NOT NULL AUTO_INCREMENT,
  `code` varchar(50) NOT NULL,
  `number` varchar(50) NOT NULL,
  `report` varchar(50) NOT NULL,
  `title` varchar(50) NOT NULL DEFAULT '',
  `amount` varchar(50) NOT NULL DEFAULT '',
  `action_options` varchar(100) NOT NULL DEFAULT '',
  `active_amount` varchar(50) NOT NULL DEFAULT '',
  `power_old_device` varchar(50) NOT NULL DEFAULT '',
  `power_new_device` varchar(50) NOT NULL DEFAULT '',
  `time_year_work` varchar(50) NOT NULL DEFAULT '',
  `electricity_tariff` varchar(50) NOT NULL DEFAULT '',
  `cost_of_implementing` varchar(50) NOT NULL DEFAULT '',
  `action_options2` varchar(100) NOT NULL DEFAULT '',
  `tariff_for_heat` varchar(50) NOT NULL DEFAULT '',
  `cost_of_implementing2` varchar(50) NOT NULL DEFAULT '',
  `number_install_screens` varchar(50) NOT NULL DEFAULT '',
  `window_area` varchar(50) NOT NULL DEFAULT '',
  `window_resistance` varchar(50) NOT NULL DEFAULT '',
  `building_wall` varchar(50) NOT NULL DEFAULT '',
  `facilities_area` varchar(50) NOT NULL DEFAULT '',
  `wall_resistance` varchar(50) NOT NULL DEFAULT '',
  `consumption` varchar(50) NOT NULL DEFAULT '',
  `pipeline_diameter` varchar(50) NOT NULL DEFAULT '',
  `insulation_thickness` varchar(50) NOT NULL DEFAULT '',
  `pipeline_length` varchar(50) NOT NULL DEFAULT '',
  `action_options3` varchar(100) NOT NULL DEFAULT '',
  `cost_of_implementing3` varchar(50) NOT NULL DEFAULT '',
  `electricity_tariff2` varchar(50) NOT NULL DEFAULT '',
  `power_consumption_year` varchar(50) NOT NULL DEFAULT '',
  `electricity_payment_before` varchar(50) NOT NULL DEFAULT '',
  `electricity_payment_after` varchar(50) NOT NULL DEFAULT '',
  `refrigerator_power` varchar(50) NOT NULL DEFAULT '',
  `power_consumption_month` varchar(50) NOT NULL DEFAULT '',
  `action_options4` varchar(100) NOT NULL DEFAULT '',
  `cost_of_implementing4` varchar(50) NOT NULL DEFAULT '',
  `electricity_tariff3` varchar(50) NOT NULL DEFAULT '',
  `hot_water_tariff` varchar(50) NOT NULL DEFAULT '',
  `cold_water_tariff` varchar(50) NOT NULL DEFAULT '',
  `time_brushing` varchar(50) NOT NULL DEFAULT '',
  `text` text NOT NULL,
  PRIMARY KEY (`id`),
  KEY `code` (`code`),
  KEY `number` (`number`),
  KEY `report` (`report`)
) ENGINE=InnoDB AUTO_INCREMENT=2479 DEFAULT CHARSET=utf8
